    "sequelize": "6.32.1",
    "sequelize-typescript": "2.1.5"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.1",
    "@types/pg": "8.6.6",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "babel-jest": "29.5.0",
    "cross-env": "7.0.3",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "jest": "29.5.0",
    "nodemon": "2.0.22",
    "prettier": "2.8.8",
